    "build": "handlebars ./views/partials/savedPlaceEntry.handlebars ./views/partials/pinInfoBox.handlebars ./views/partials/pinInfoBoxReadOnly.handlebars ./views/partials/pins.handlebars -f ./public/handlebarsPartials.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanthayer/PlaceKeeper.git"
  },
