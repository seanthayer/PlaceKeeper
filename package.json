{
  "name": "PlaceKeeper",
  "version": "1.0.0",
  "description": "PlaceKeeper is an application which utilizes the Google Maps JavaScript API to allow the user to store points of interest.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "npm run build",
    "start": "node server.js",
    "predev": "npm run build",
