  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seanthayer/PlaceKeeper/issues"
  },
  "homepage": "https://github.com/seanthayer/PlaceKeeper#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "handlebars": "^4.7.6"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
